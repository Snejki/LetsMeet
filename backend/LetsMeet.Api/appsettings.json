{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SeriLog": {
    "File": {
      "Enabled": false,
      "FilePath": "logs/today.log",
      "LogTemplate": "[{Timestamp:HH:mm:ss} {CorrelationId} {Level:u3}] {Message:lj}{NewLine}{Exception}"
    },
    "Seq": {
      "Enabled": true,
      "ServerUrl": "http://localhost:5342/"
    }
  },
  "AllowedHosts": "*"
}
